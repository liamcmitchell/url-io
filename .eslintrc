{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": ["warn", {"allow": ["warn", "error"]}]
  },
  "plugins": ["jest"],
  "ignorePatterns": ["dist/**"]
}
