{
	"extends": ["eslint:recommended"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
	},
	"env": {
		"es6": true,
		"browser": true
	},
	"rules": {
		"semi": [2, "never"],
		"no-trailing-spaces": 1,
		"no-console": ["warn", { "allow": ["warn", "error"]}]
	}
}
