{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "no-console": ["warn", {"allow": ["warn", "error"]}]
  },
  "plugins": ["jest", "@typescript-eslint"],
  "ignorePatterns": ["dist/**"],
  "root": true
}
