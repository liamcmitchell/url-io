{
  "extends": ["eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"]}]
  },
  "plugins": [
    "jest"
  ]
}
