{
  "name": "url-io",
  "version": "4.1.6",
  "description": "Data request API",
  "files": [
    "dist"
  ],
  "main": "dist/url-io.js",
  "module": "dist/url-io.es.js",
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "onchange 'modules/**/*.js' -- rollup -c",
    "test": "jest",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "prepush": "npm test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "eslint"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/url-io.git"
  },
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/url-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/url-io#readme",
  "dependencies": {
    "lodash": "^4.17.5",
    "rxjs": "^5.5.6"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.12.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^7.0.0",
    "onchange": "^3.3.0",
    "prettier": "1.11.1",
    "rimraf": "^2.5.2",
    "rollup": "^0.56.3"
  }
}
