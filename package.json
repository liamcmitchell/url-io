{
  "name": "url-io",
  "version": "4.1.3",
  "description": "Data request API",
  "files": [
    "es6",
    "lib"
  ],
  "main": "lib/index",
  "module": "es6/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./modules -d lib --ignore '__tests__'",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./modules -d es6 --ignore '__tests__'",
    "dev": "npm run build-es && fswatch -o modules | xargs -n1 -I{} cross-env BABEL_ENV=es babel ./modules -d es6 --ignore '__tests__'",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/url-io.git"
  },
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/url-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/url-io#readme",
  "dependencies": {
    "lodash": "^4.17.4",
    "rxjs": "^5.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^21.2.1",
    "rimraf": "^2.5.2"
  }
}
