{
  "name": "url-io",
  "version": "4.0.1",
  "description": "Data request API",
  "files": [
    "es6",
    "lib"
  ],
  "main": "lib/index",
  "module": "es6/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./modules -d lib --ignore '__tests__'",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./modules -d es6 --ignore '__tests__'",
    "dev": "npm run build-es && fswatch -o modules | xargs -n1 -I{} cross-env BABEL_ENV=es babel ./modules -d es6 --ignore '__tests__'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/url-io.git"
  },
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/url-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/url-io#readme",
  "dependencies": {
    "lodash": "^4.13.1",
    "rxjs": "^5.0.0",
    "symbol-observable": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.0.0",
    "rimraf": "^2.5.2"
  }
}
