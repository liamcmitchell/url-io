{
  "name": "url-io",
  "version": "5.2.0",
  "description": "Data request API",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-io.mjs",
      "require": "./dist/react-io.cjs"
    }
  },
  "main": "./dist/react-io.cjs",
  "side-effects": false,
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "dev": "rollup -c --watch",
    "test": "jest",
    "prepare": "npm run build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liamcmitchell/url-io.git"
  },
  "author": "Liam Mitchell",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liamcmitchell/url-io/issues"
  },
  "homepage": "https://github.com/liamcmitchell/url-io#readme",
  "dependencies": {
    "lodash": "^4.17.19"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-babel": "^6.0.4",
    "babel-jest": "^29.7.0",
    "benchmark": "^2.1.4",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "history": "^5.0.0",
    "husky": "^4.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.4.1",
    "rxjs": "^7.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  }
}
